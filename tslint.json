{
    "defaultSeverity": "default",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "interface-name": false,
        "ordered-imports": false,
        "trailing-comma": false,
        "member-access": false,
        "quotemark": {
            "options": "single"
        },
        "object-literal-sort-keys": false,
        "no-console": false,
        "arrow-parens": false,
        "member-ordering": [
            true,
            {
                "order": "instance-sandwich"
            }
        ],
        "no-unused-expression": true,
        "no-unused-variable": true,
        "variable-name": [
            true,
            "ban-keywords",
            "check-format",
            "allow-leading-underscore"
        ],
        "completed-docs": {
            "options": {
                "interfaces": {
                    "visibilities": "all"
                },
                "classes": {
                    "visibilities": "all"
                }
            }
        }
    },
    "rulesDirectory": []
}
